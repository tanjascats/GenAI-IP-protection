	note = {ISSN: 2379-190X},
	keywords = {Codes, DeepFake, Deepfakes, Generators, Semantics, Signal processing, Task analysis, Visualization, adversarial attack, deepfake, image forensics, latent representations, proactive defense},
	pages = {4750--4754},
}

